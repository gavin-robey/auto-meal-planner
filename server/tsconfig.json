{
    // "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2016",
        "noImplicitAny": true,
        "removeComments": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "rootDir": "./src",
        "outDir": "./dist",
        "baseUrl": ".",
        "paths": {
            "src/*": ["./src/*"]
        },
        "jsx": "react-jsx",
        "lib": ["ES2016", "DOM"]
    }
}